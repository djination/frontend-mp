# Default configuration (akan di-override oleh file environment spesifik)
VITE_ENV=development
NODE_ENV=development

# API Configuration
VITE_BASE_URL=http://localhost:5000
VITE_API_BASE_URL=http://localhost:5000
VITE_WS_URL=ws://localhost:5000

# HMR Configuration - WebSocket terpisah dari main server
VITE_DISABLE_HMR=false
VITE_HMR_HOST=127.0.0.1
VITE_HMR_PORT=5173
VITE_HMR_CLIENT_PORT=443
VITE_HMR_PROTOCOL=wss
VITE_HMR_WS_URL=wss://localhost:5173

# Debug mode
VITE_DEBUG=true
