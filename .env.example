# Default configuration (akan di-override oleh file environment spesifik)
VITE_ENV=development
NODE_ENV=development

# API Configuration
VITE_BASE_URL=http://127.0.0.1:5000
VITE_API_BASE_URL=http://127.0.0.1:5000
VITE_WS_URL=wss://127.0.0.1:5000

# HMR Configuration - WebSocket terpisah dari main server
VITE_DISABLE_HMR=true
VITE_HMR_HOST=127.0.0.1
VITE_HMR_PORT=5173
VITE_HMR_CLIENT_PORT=443
VITE_HMR_PROTOCOL=wss
VITE_HMR_WS_URL=wss://127.0.0.1


# Debug mode
VITE_DEBUG=true

REACT_APP_OAUTH_CLIENT_ID=bmp-admin-credential-id
REACT_APP_OAUTH_CLIENT_SECRET=bmp-admin-credential-secret
REACT_APP_OAUTH_TOKEN_URL=https://stg.merahputih-id.tech:9002/oauth/token

# Also provide VITE_ prefixed versions for compatibility
VITE_OAUTH_CLIENT_ID=bmp-admin-credential-id
VITE_OAUTH_CLIENT_SECRET=bmp-admin-credential-secret
REACT_APP_EXTERNAL_API_BASE_URL=https://stg.merahputih-id.tech:5002/api

# Audit Trail Service Token
VITE_AUDIT_SERVICE_TOKEN=audit-service-2024-secure-token-merahputih