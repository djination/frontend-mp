# Development environment configuration untuk server dengan nginx
VITE_ENV=development
NODE_ENV=development

# API Configuration untuk development
VITE_BASE_URL=https://customer.merahputih-id.com
VITE_API_BASE_URL=https://bc.merahputih-id.com
VITE_WS_URL=wss://bc.merahputih-id.com

