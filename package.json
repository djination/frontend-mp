{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode local",
    "dev:server": "vite --mode server",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:server": "vite build --config vite.config.production.js --mode production",
    "build:esbuild": "./scripts/build-esbuild.sh",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "deploy:auto": "./scripts/server-deploy.sh",
    "fix:nginx": "./scripts/nginx-fix.sh",
    "start:dev": "./scripts/start-dev.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "start": "serve -s dist -l 3000",
    "switch:local": "copy .env.local .env.active",
    "switch:server": "copy .env.server .env.active",
    "switch:prod": "copy .env.production .env.active"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "antd": "^5.26.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "postcss": "^8.5.5",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "serve": "^14.2.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0"
  }
}
